<h1>Listing rols</h1>

<table>
  <thead>
    <tr>
      <th>Rol name</th>
      <th>Admin</th>
      <th>Module 1</th>
      <th>Module 2</th>
      <th>Module 3</th>
      <th>Module 4</th>
      <th>Module 5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rols.each do |rol| %>
      <tr>
        <td><%= rol.rol_name %></td>
        <td><%= rol.admin %></td>
        <td><%= rol.module_1 %></td>
        <td><%= rol.module_2 %></td>
        <td><%= rol.module_3 %></td>
        <td><%= rol.module_4 %></td>
        <td><%= rol.module_5 %></td>
        <td><%= link_to 'Show', rol %></td>
        <td><%= link_to 'Edit', edit_rol_path(rol) %></td>
        <td><%= link_to 'Destroy', rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol', new_rol_path %>
